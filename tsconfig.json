{
	"compilerOptions": {
		"target": "ES5",
		"lib": [
			"es2015"
		],
		"module": "commonjs",

		"noImplicitAny": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"strictBindCallApply": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"downlevelIteration": true,

		"allowJs": true,
		"checkJs": true
	}
}
